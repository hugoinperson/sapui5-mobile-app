{
	"_version": "1.5.0",
	"sap.app": {
		"id": "ui5.mobile.app",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "0.0.1"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"

		]
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "ui5.mobile.app.view.Index",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ui5.mobile.app.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"async": true,
				"clearControlAggregation": true,
				"viewType": "XML",
				"viewPath": "ui5.mobile.app.view"
			},
			"routes": {
				"login": {
					"pattern": "",
					"target": "login"
				},
				"payment": {
					"pattern": "payment",
					"target": ["mainNav", "payment"]
				},
				"notPhone": {
					"pattern": "notPhone",
					"target": "notPhone"
				},
				"error": {
					"pattern": "",
					"target": "error"
				}
			},
			"targets": {
				"login": {
					"viewName": "Login",
					"controlAggregation": "content",
					"controlId": "splashPage"
				},
				"mainNav": {
					"viewName": "MainNav",
					"controlAggregation": "headerContent",
					"controlId": "mainPage"
				},
				"payment": {
					"viewName": "payment.main",
					"controlAggregation": "content",
					"controlId": "mainPage"
				},
				"notPhone": {
					"viewName": "NotPhone",
					"controlAggregation": "content",
					"controlId": "splashPage"
				},
				"error": {
					"viewName": "Error",
					"controlAggregation": "content",
					"controlId": "splashPage"
				}
			}
		}
	}
}